{
  "name": "ThreaditJS_react-redux",
  "version": "1.0.0",
  "description": "Implementation of ThreaditJS reference app using React & Redux",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --content-base dist/",
    "test": "sh -c 'flow && NODE_ENV=test mocha --recursive --compilers js:babel-register'",
    "test:watch": "npm test -- --watch",
    "typecheck": "flow",
    "build": "webpack"
  },
  "repository": "https://github.com/koglerjs/threaditjs",
  "author": "Jesse Hallett <jesse@sitr.us>",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "6.9.0",
    "babel-runtime": "6.6.1",
    "react": "~15.0.1",
    "react-dom": "15.0.1",
    "react-redux": "4.4.5",
    "react-router": "2.3.0",
    "react-router-redux": "4.0.4",
    "redux": "3.5.2",
    "redux-crud-store": "4.0.0",
    "redux-form": "5.2.5",
    "redux-logger": "2.6.1",
    "redux-saga": "0.10.5"
  },
  "devDependencies": {
    "babel-core": "^6.7.7",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-class-properties": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "expect": "^1.20.1",
    "flow-bin": "^0.26.0",
    "mocha": "^2.5.3",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  },
  "babel": {
    "presets": [
      "react",
      "es2015"
    ],
    "plugins": [
      "transform-class-properties",
      "transform-object-rest-spread"
    ]
  }
}
